package exercicio_fix;

import javax.swing.JOptionPane;
/**
 *
 * @author Discente
 */

public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

   double x,y,res;
   String sx,sy;

  sx=JOptionPane.showInputDialog(null,"Valor de x: ");
    sy=JOptionPane.showInputDialog(null,"Valor de y: "); 
    
        x=Double.parseDouble(sx);
   y=Double.parseDouble(sy);
   
   res= Math.pow(x, 2)-(3*y)+1;
   
   if(y>res){JOptionPane.showMessageDialog(null,"Ponto acima da parábola");} else 
   if(y<res){JOptionPane.showMessageDialog(null,"Ponto abaixo da parábola");} else
       {JOptionPane.showMessageDialog(null,"Ponto pertencente a parábola");}
   
   
    }
   
}
